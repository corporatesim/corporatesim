CREATE TABLE GAME_JSGAME( Js_id INT PRIMARY KEY AUTO_INCREMENT, Js_Name VARCHAR(250) COMMENT "Name of JS game", Js_GameId INT COMMENT "This is game id from game_game table", Js_ScenId INT COMMENT "This is scenario id from game_scenario table", Js_LinkId INT COMMENT "This is link id for game and scenario", Js_Element VARCHAR(250) COMMENT "this is the default elements of game, described in js code", Js_Alias VARCHAR(250) COMMENT "alias for the js game elements", Js_SublinkId LONGTEXT DEFAULT NULL COMMENT 'This is the mapped sublink id, which contains component id', Js_CreatedOn DATETIME DEFAULT CURRENT_TIMESTAMP, Js_CreatedBy INT DEFAULT NULL, Js_UpdatedOn DATETIME DEFAULT NULL, Js_UpdatedBy INT DEFAULT NULL, Js_Status TINYINT DEFAULT 0 COMMENT "0-Active, 1-Deactive" );

