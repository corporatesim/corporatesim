CREATE TABLE GAME_USER_JSGAME_DATA( JSDATA_Id INT AUTO_INCREMENT PRIMARY KEY, JSDATA_JsGameId INT DEFAULT NULL COMMENT "This is js game id", JSDATA_JsGameName INT DEFAULT NULL COMMENT "This is js game name", JSDATA_GameId INT DEFAULT NULL COMMENT "This is simulation game id", JSDATA_ScenId INT DEFAULT NULL COMMENT "This is scenario id", JSDATA_LinkId INT DEFAULT NULL COMMENT "This is simulation game and scenario link id", JSDATA_UserId INT DEFAULT NULL COMMENT "This is User id", JSDATA_GameData LONGTEXT DEFAULT NULL COMMENT "This contains all the js game input data of the user, in json format", JSDATA_CreatedOn DATETIME DEFAULT CURRENT_TIMESTAMP )

