ALTER TABLE GAME_INPUT ADD input_updatedOn DATETIME DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP AFTER input_caretedOn;
ALTER TABLE GAME_LINKAGE ADD Link_UpdatedOn DATETIME DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP;
ALTER TABLE `GAME_LINKAGE_TIMER` CHANGE `timer` `timer` DECIMAL(10,3) NOT NULL;
ALTER TABLE `GAME_LINKAGE` CHANGE `Link_Min` `Link_Min` DECIMAL(10,3) NOT NULL;
UPDATE GAME_LINKAGE_TIMER glt INNER JOIN GAME_LINKAGE gl ON gl.Link_ID = glt.linkid SET glt.timer = ROUND( ( gl.Link_Hour * 60 + gl.Link_Min ), 2 )



CREATE TABLE GAME_LINK_CHANGES( Glc_Id INT AUTO_INCREMENT PRIMARY KEY, Glc_GameId INT COMMENT 'this is the game id', Glc_ScenId INT COMMENT 'this is the scenario id', Glc_UserName VARCHAR(250), Glc_Hour VARCHAR(60), Glc_Min VARCHAR(120), Glc_Json LONGTEXT COMMENT 'This holds all the parameter of scenario linkage, keep it for later testing purpose', Glc_CreatedOn DATETIME DEFAULT CURRENT_TIMESTAMP, Glc_CreatedBy INT COMMENT "This is admin user id", Glc_Status VARCHAR(60) DEFAULT NULL COMMENT 'added or changed/updated' )

////////////////////////////// To show the game link changes
SELECT gg.Game_Name, gs.Scen_Name, glc.* FROM GAME_LINKAGE gl JOIN GAME_SCENARIO gs ON gs.Scen_ID = gl.Link_ScenarioID JOIN GAME_GAME gg ON gg.Game_ID = gl.Link_GameID JOIN GAME_LINK_CHANGES glc ON glc.Glc_GameId = glc.Glc_GameId AND glc.Glc_ScenId = gl.Link_ScenarioID WHERE gl.Link_ScenarioID = 87 ORDER BY glc.Glc_Id


CREATE TABLE GAME_APP_LOG( Gal_Id INT AUTO_INCREMENT PRIMARY KEY, Gal_UserId INT, Gal_UserData VARCHAR(255) COMMENT "This holds the fname, lname, username and email", Gal_GameId INT, Gal_ScenId INT, Gal_LinkId INT, Gal_CompId INT, Gal_CompName VARCHAR(160), Gal_SubCompId INT, Gal_SubCompName VARCHAR(160), Gal_ErrorMsg LONGTEXT, Gal_CustomErrorMsg LONGTEXT, Gal_CreatedOn DATETIME DEFAULT CURRENT_TIMESTAMP )

SELECT gg.Game_Name, gs.Scen_Name, Glc_UserName, glc.Glc_Hour, glc.Glc_Min, glc.Glc_Json, glc.Glc_CreatedOn, glc.Glc_Status, ((glc.Glc_Hour * 60) + glc.Glc_Min) AS TotalMinutes FROM GAME_LINK_CHANGES glc LEFT JOIN GAME_GAME gg ON gg.Game_ID = glc.Glc_GameId LEFT JOIN GAME_SCENARIO gs ON gs.Scen_ID = Glc_ScenId ORDER BY gg.Game_Name, glc.Glc_CreatedOn

SELECT gg.Game_Name, gs.Scen_Name, gl.Link_Hour, gl.Link_Min, glt.timer, CONCAT( gsu.User_fname, ' ', gsu.User_lname ) AS userName, ( (gl.Link_Hour * 60) + gl.Link_Min ) AS CurrentTImeInMin FROM GAME_LINKAGE_TIMER glt LEFT JOIN GAME_SITE_USERS gsu ON gsu.User_id = glt.userid LEFT JOIN GAME_LINKAGE gl ON gl.Link_ID = glt.linkid LEFT JOIN GAME_GAME gg ON gg.Game_ID = gl.Link_GameID LEFT JOIN GAME_SCENARIO gs ON gs.Scen_ID = gl.Link_ScenarioID WHERE gsu.User_id IN( 12056, 12057, 12058, 12059, 12060, 12061, 12062, 12063, 7574, 7575 ) AND gg.Game_Name IS NOT NULL ORDER BY gsu.User_id, gg.Game_Name


SELECT gg.Game_Name,gs.Scen_Name,glt.*, concat(gsu.User_fname,' ', gsu.User_lname, ' (', gsu.User_email,')') as UserData FROM `GAME_LINKAGE_TIMER` glt LEFT JOIN GAME_LINKAGE gll ON gll.Link_ID=linkid LEFT JOIN GAME_GAME gg ON gg.Game_ID=gll.Link_GameID LEFT JOIN GAME_SCENARIO gs ON gs.Scen_ID=gll.Link_ScenarioID LEFT JOIN GAME_SITE_USERS gsu ON gsu.User_id=userid WHERE userid IN( 1989, 12056, 12057, 12058, 12059, 12060, 12061, 12062, 12063, 7574, 7575,4054,12054,9846 ) AND linkid IN( SELECT gl.Link_ID FROM GAME_LINKAGE gl WHERE gl.Link_GameID IN(48, 47, 69, 154, 155) AND gl.Link_Status = 1 ) ORDER BY userid, gg.Game_Name, gs.Scen_Name

