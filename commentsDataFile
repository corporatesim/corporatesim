INSERT INTO GAME_VIEWS( `views_Game_ID`, `views_sublinkid`, `views_key` ) SELECT '90' AS views_Game_ID, gls.SubLink_ID AS views_sublinkid, IF( gv.views_id IS NULL, CONCAT( gls.SubLink_AreaName, '_', IF( gls.SubLink_SubCompID, CONCAT('subc', '_', gls.SubLink_SubCompID), CONCAT('comp', '_', gls.SubLink_CompID) ) ), 'not insert' ) AS views_key FROM GAME_LINKAGE_SUB gls LEFT JOIN GAME_VIEWS gv ON gv.views_sublinkid = gls.SubLink_ID AND gv.views_Game_ID = 90 WHERE gls.SubLink_LinkID IN( SELECT gl.Link_ID FROM GAME_LINKAGE gl WHERE gl.Link_GameID = 90 ) AND gv.views_id IS NULL ORDER BY `gv`.`views_sublinkid`, gls.SubLink_LinkID, gls.SubLink_SubCompID ASC;

